{
  "name": "install-module-linked",
  "version": "1.1.3",
  "description": "Installs and symlinks a module into node_modules",
  "keywords": [
    "npm",
    "install",
    "module",
    "cache",
    "symlink",
    "link"
  ],
  "homepage": "https://github.com/kmalakoff/install-module-linked#README.md",
  "bugs": {
    "url": "https://github.com/kmalakoff/install-module-linked/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/install-module-linked.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kevin Malakoff",
    "email": "kmalakoff@gmail.com",
    "url": "https://github.com/kmalakoff"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "bin": {
    "iml": "./bin/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "docs": "tsds docs",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^2.0.13",
    "exit": "^0.1.2",
    "fs-access-compat": "^1.1.4",
    "get-remote": "^1.3.9",
    "getopts-compat": "^2.2.5",
    "homedir-polyfill": "^1.0.3",
    "mkdirp-classic": "^0.5.3",
    "queue-cb": "^1.4.10",
    "rimraf2": "^2.8.2",
    "temp-suffix": "^0.1.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "pinkie-promise": "^2.0.1"
  },
  "engines": {
    "node": ">=0.10"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
